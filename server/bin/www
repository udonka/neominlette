#!/usr/bin/env node
var debug = require('debug')('minlette');
var app = require('../app');
var sio = require('../models/sio');

app.set('port', process.env.PORT || 6969);

var server = app.listen(app.get('port'), function() {
  debug('Express server listening on port ' + server.address().port);
});

sio(server);
