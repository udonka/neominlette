<!doctype html>
<html lang="ja">
<% include head %>
<body>
<% include top %>
<div id="wrapper" class="container"> 
  <div class="m-content">
    <form action="/login" method="POST" class="m-form" >
      <div class="form-group">
        <label for="inputEmail">Email</label>
        <input type="email" class="form-control" id="inputEmail" name="email" placeholder="Enter Email">
      </div>
      <div class="form-group">
        <label for="inputPassword">Password</label>
        <input id="inputPassword" class="form-control" type="password" name="password" placeholder="Password">
      </div>
      <button type="submit" class="btn btn-default">Login</button>
    </form>
  </div>

  <form action="/signup" method="GET" class="m-form">
    <div class="form-group">
      <button type="submint" class="btn btn-default" id="toAdd" >Create</button>
    </div>
  </form>
</div>
</body>
</html>
